{
    "name": "clickhouse-sink-connect",
    "config": {
        "connector.class": "com.clickhouse.kafka.connect.ClickHouseSinkConnector",
        "tasks.max": "1",
        "consumer.override.max.poll.records": "5000",
        "consumer.override.max.partition.fetch.bytes": "5242880",
        "errors.retry.timeout": "60",
        "exactlyOnce": "false",
        "topics": "mysql_server.cdc.test_table",
        "topic2TableMap": "mysql_server.cdc.test_table=test_table",
        "hostname": "clickhouse",
        "database": "cdc",
        "username": "default",
        "password": "default_password",
        "port": "8123",
        "value.converter.schemas.enable": "true",
        "key.converter.schemas.enable": "true",
        "value.converter": "io.confluent.connect.avro.AvroConverter",
        "key.converter": "io.confluent.connect.avro.AvroConverter",
        "value.converter.schema.registry.url":"http://localhost:8080",
        "key.converter.schema.registry.url":"http://localhost:8080",
        "clickhouseSettings": ""
    }
}
